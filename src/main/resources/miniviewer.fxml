         <Label fx:id="taskDay" layoutX="10.0" layoutY="156.0" prefHeight="76.0" prefWidth="432.0" text="// day of week" textFill="#2082bf" />
         <Label fx:id="taskStart" layoutX="14.0" layoutY="232.0" prefHeight="34.0" prefWidth="439.0" text="//start" textFill="#2082bf" />
         <Label fx:id="taskDuration" layoutX="10.0" layoutY="266.0" prefHeight="53.0" prefWidth="432.0" text="// duration" textFill="#2082bf" />
         <Label fx:id="taskStatus" layoutX="13.0" layoutY="319.0" prefHeight="31.0" prefWidth="441.0" text="// status" textFill="#2082bf" />
      </children></AnchorPane>
</Scene>
